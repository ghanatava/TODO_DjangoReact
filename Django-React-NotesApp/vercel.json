{
    "version": 2,
    "builds": [
      {
        "src": "frontend/",  
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"  
        }
      },
      {
        "src": "backend/**",  
        "use": "@vercel/python"
      }
    ],
    "routes": 
    [
      {
        "src": "/api/(.*)",  
        "dest": "backend/api/$1",  
        "methods": ["GET", "POST", "PUT", "DELETE"]  
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1" 
    ]
  }
  